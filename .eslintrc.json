{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": 0,
        "no-console": 0,
        "comma-dangle": 0,
        "import/extensions": 0,
        "import/prefer-default-export": 0,
        "camelcase": 0,
        "no-underscore-dangle": 0,
        "func-names": 0
    }
}
